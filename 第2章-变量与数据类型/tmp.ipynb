   "source": [
    "a = 10       # 二进制表示为 1010\n",
    "b = 4        # 二进制表示为 0100\n",
    "print(a & b) # 输出：0，二进制中对应位都是1才为1，1010 & 0100 = 0000\n",
    "print(a | b) # 输出：14，二进制中对应位至少有一个1即为1，1010 | 0100 = 1110，即14\n",
    "print(~a)    # 输出：-11，按位取反，1010变为0101，负数用补码表示，0101的补码是1011,即-11\n"
   ]
  }
